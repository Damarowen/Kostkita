<%- include ('./partials/header')  %>


<div class="container container-form">
    <div class="info">
        <h2>Hi There,</h2>
        <h2>This Is Admin Login Page</h2>
    </div>

  
        <form action="/adminlogin" novalidate class="validated-form" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" placeholder="Admin username" name="username" required>
            </div>

            <div class="form-group">
                <input class="form-control" type="password" placeholder="Admin password" name="password" required>
            </div>

            <div class="form-group">
                <button class="btn btn-lg btn-pink btn-block">Submit!</button>
            </div>
            <a href="/campground">Go Back</a>
     </form>
       

    
</div>



